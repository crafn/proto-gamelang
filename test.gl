let g_value : int = 8;

let fibo := fn (n : int) -> int
{
	if (n == 0) {
		return 0;
	}
	if (n == 1) {
		return 1;
	}
	return fibo(n - 1) + fibo(n - 2);
};

let Kana := struct {
	var age : int       = 0;
	var mass : int      = fibo(g_value);
	var parent : ?Kana  = null;
};


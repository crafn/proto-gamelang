let g_value : int = 8;

let fibo := fn (n : int) -> int
{
	if (n == 0) {
		return 0;
	}
	if (n == 1) {
		return 1;
	}
	return fibo(n - 1) + fibo(n - 2);
}

let Kana := struct
{
	var age : int = fibo(g_value);
	var mass : int = 0;
}

let gotoExample := fn () -> int
{
	var i : int = 0;
	test_label:
		i = i + 1;	
		if (i != 99) {
			goto test_label;
		}
	return i;
}

let main := fn () -> int
{
	var kana : Kana; // Ctor is called
	kana.mass = 1995;
	return kana.age;
}


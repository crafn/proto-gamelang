let g_value : int = 11;

let fibo := fn (n : int) -> int
{
	if (n == 0) {
		return 0;
	}
	if (n == 1) {
		return 1;
	}
	return fibo(n - 1) + fibo(n - 2);
}

let Kana := struct
{
	var age : int = 0;
	var mass : int = fibo(5);
}

let main := fn () -> int
{
	var kana : Kana;
	kana.mass = 1995;

	var a : int;
	a = fibo(g_value + 1);
	return a;
}

